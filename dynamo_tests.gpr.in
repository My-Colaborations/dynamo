with "dynamo";

project Dynamo_Tests is

   Mains := ("dynamo_harness.adb");
   for Main use Mains;
   for Source_Dirs use ("regtests");
   for Object_Dir use "./" & Config'Object_Dir & "/obj";
   for Exec_Dir use "./" & Config'Exec_Dir & "/bin";

   package Binder renames Config.Binder;
