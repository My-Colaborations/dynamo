with "config";
with "@UTIL_DIR@util";
with "@UTIL_DIR@testutil";
with "@EL_DIR@el";
with "@ASF_DIR@asf";
with "@ADO_DIR@ado";

project Check_Build is

   Version := "0.1.0";
   Library_Dir := "lib";
   Library_Type : Config.Library_Type_Type := external ("LIBRARY_TYPE", "static");

   for Library_Name use "ada-tst";
   for Library_Kind use Library_Type;

   for Source_Dirs use ("src/model", "../regtests/check_build");
   
   for Library_Dir use Library_Dir & "/check-code/" & Project'Library_Kind;
   for Object_Dir use "./" & Config'Object_Dir & "/obj/check-code/" & Project'Library_Kind;
   for Exec_Dir use "./" & Config'Exec_Dir & "/bin";

   package Binder renames Config.Binder;
   package Builder renames Config.Builder;
   package Compiler renames Config.Compiler;
   package Ide      renames Config.Ide;

end Check_Build;
